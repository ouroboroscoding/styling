{
  "name": "@ouroboros/styling",
  "version": "0.1.1",
  "description": "CSS for Ouroboros Coding projects",
  "main": "oc-styling.css",
  "scripts": {
    "build": "sass --no-source-map --load-path=node_modules --style=compressed src/index.scss oc-styling.min.css",
    "start": "sass --watch --update --load-path=node_modules --style=expanded src/index.scss oc-styling.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ouroboroscoding/styling"
  },
  "keywords": [
    "Cascading Style Sheets",
    "SASS",
    "CSS"
  ],
  "author": "Chris Nasr <chris@ouroboroscoding.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ouroboroscoding/styling/issues"
  },
  "homepage": "https://github.com/ouroboroscoding/styling",
  "devDependencies": {
    "sass": "^1.86.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}